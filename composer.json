{
    "name": "laravelcollective/auth",
    "description": "Auth backend and Controller stubs for the Laravel Framework from Laravel UI",
    "license": "MIT",
    "homepage": "https://laravelcollective.com",
    "support": {
        "issues": "https://github.com/LaravelCollective/auth/issues",
        "source": "https://github.com/LaravelCollective/auth"
    },
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "matt@laravelcollective.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/filesystem": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^6.23|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Collective\\Auth\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Collective\\Auth\\CollectiveAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
